    "from pytube import YouTube\n",
    "link = input(\"Enter link:  \")\n",
    "connector = YouTube(link)\n",
    "connector = connector.streams.get_highest_resolution()\n",
    "try:\n",
    "    connector.download()\n",
    "except:\n",
    "    print(\"Error\")\n",
    "print(\"Dowloaded\")"
   ]
